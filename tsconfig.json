{
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types"
    ],
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": ["/*"],
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "./content/**/*.ts",
    "./content/**/*.tsx",
    "./.next/types/**/*.ts",
    "next-env.d.ts",
    "build/types/**/*.ts"
, "homepage/intro.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
